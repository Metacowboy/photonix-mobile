{
  "expo": {
    "name": "Photonix",
    "slug": "photonix-mobile",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "description": "Organize and access your photos. Client application to be used in conjunction with a Photonix server installation.",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d1d1d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.photonix.mobile"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "android": {
      "package": "org.photonix.mobile"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "githubUrl": "https://github.com/damianmoore/photonix-mobile"
  }
}
