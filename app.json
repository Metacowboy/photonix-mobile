{
  "expo": {
    "name": "Photonix",
    "slug": "photonix-mobile",
    "version": "0.1.2",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "description": "Organize and access your photos. Client application to be used in conjunction with a Photonix server installation.",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d1d1d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "org.photonix.mobile",
      "versionCode": 3,
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "org.photonix.mobile",
      "buildNumber": "1",
      "icon": "./assets/icon_ios.png",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "githubUrl": "https://github.com/damianmoore/photonix-mobile"
  }
}
